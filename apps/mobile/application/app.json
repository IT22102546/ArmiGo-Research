{
  "expo": {
    "name": "LearnApp",
    "slug": "faite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LearnUpLogo.png",
    "scheme": "learnapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.learnapp.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow this app to access your photo library to upload images."
      }
    },
    "android": {
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LearnUpLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.learnup.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/LearnUpLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LearnUpLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "gradlePluginVersion": "8.3.1",
            "minSdkVersion": 24,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "kotlinVersion": "2.0.21",  
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8ad0f96f-f73e-467b-a809-c37d49ce50d7"
      }
    },
    "owner": "sanjananimesh2001",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}