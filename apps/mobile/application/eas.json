{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "EXPO_PUBLIC_NODE_STORAGE_URL": "http://192.168.117.109:3000/uploads",
        "EXPO_PUBLIC_API_KEY": "http://192.168.117.109:3000"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_NODE_STORAGE_URL": "http://192.168.117.109:3000/uploads",
        "EXPO_PUBLIC_API_KEY": "http://192.168.117.109:3000"
      }
    },
    "production": {
      "android": {
        "buildType": "aapp-bundle",
        "gradleCommand": ":pp:bundleRelease"
        
      },
      "env": {
        "EXPO_PUBLIC_NODE_STORAGE_URL": "https://learnappbc.obtaste.com/uploads",
        "EXPO_PUBLIC_API_KEY": "https://learnappbc.obtaste.com"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
